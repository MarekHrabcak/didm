  "Oidc": {
    "Authority": "http://keycloak:8090/realms/master",
    "ClientId": "agent",
    "RequireHttpsMetadata": false
  },
  "OpenIdConnect": {
    "AuthenticationProperties": {
      
    }
  }
}
